<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="BodyProperties" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="BodyProperties" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="BodyProperty" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:all>
              <xs:element name="BodyPropertiesMin">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute type="xs:string" name="age" use="required"/>
                      <xs:attribute type="xs:string" name="weight" use="required"/>
                      <xs:attribute type="xs:string" name="build" use="required"/>
                      <xs:attribute type="xs:string" name="version" use="required"/>
                      <xs:attribute type="xs:string" name="key" use="required"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="BodyPropertiesMax">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute type="xs:string" name="age" use="required"/>
                      <xs:attribute type="xs:string" name="weight" use="required"/>
                      <xs:attribute type="xs:string" name="build" use="required"/>
                      <xs:attribute type="xs:string" name="version" use="required"/>
                      <xs:attribute type="xs:string" name="key" use="required"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:all>
            <xs:attribute type="xs:string" name="id" use="required"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>